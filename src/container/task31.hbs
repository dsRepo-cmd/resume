<main class='ms-4 mt-4'>
  <section class='row g-3'>
    {{! _________________________________________________________________________________________ }}
    {{#with navigation}}
      <div class='col-12 pt-3'>
        {{#each links}}
          <a
            class='me-3 text-decoration-none'
            href='{{href}}'
          >{{text}}</a>
        {{/each}}
        <a
          class='btn btn-primary btn-sm'
          href='{{button.href}}'
          target='_blank'
        >{{button.text}}</a>
      </div>
    {{/with}}
    <hr />
    {{! _Header_________________________________________________________________________ }}
    {{#with header}}
      <div>
        <div class='h1'>{{title}}</div>
        <p>{{description}}</p>
        <a
          class='btn btn-primary'
          href='{{button.link}}'
          target='_blank'
        >{{button.text}}</a>
      </div>
    {{/with}}
    <hr />
    {{! city_______________________________________________________________________________ }}
    {{#with city}}
      <div class='h2'>{{title}}</div>
      <div class='col-9'>
        <div class='row'>
          {{#each cards}}
            <div class='g-4 col-6'>
              <div class='card card h-100'>
                <img
                  class='card-img-top'
                  src='{{picture.src}}'
                  alt='{{picture.alt}}'
                  target='_blank'
                />
                <div class='card-body'>
                  <div class='h4 card-title'>
                    {{title}}
                  </div>
                  <p class='card-text'>
                    {{description}}
                  </p>
                  <a
                    href='{{button.href}}'
                    class='btn btn-primary'
                    target='_blank'
                  >{{button.text}}</a>
                </div>
              </div>
            </div>
          {{/each}}
        </div>
      </div>
    {{/with}}
    <hr />
    {{! winner_______________________________________________________________________________ }}
    {{#with winner}}
      <div class='h2'>{{title}} </div>
      {{#with table}}
        <table class='table'>
          <thead>
            <tr>
              {{#each head}}
                <th>
                  {{this}}
                </th>
              {{/each}}
            </tr>
          </thead>
          {{#each user}}
            <tbody>
              <tr>
                <th>
                  {{place}}
                </th>
                <td>
                  {{name}}
                </td>
                <td>
                  {{surname}}
                </td>
                <td>
                  {{nickname}}
                </td>
              </tr>
            </tbody>
          {{/each}}
        </table>
      {{/with}}
    {{/with}}
    <hr />
    {{! __gallery__________________________________________________________________________________ }}
    {{#with gallery}}
      <h2>{{title}}</h2>
      {{#each cards}}
        <div class='col-3 gx-2'>
          <div class='card h-100'>
            <div class='card-header h-100'>
              {{name}}
            </div>
            <ul class='list-group list-group-flush'>
              {{#each items}}
                <li class='list-group-item'>{{this}}</li>
              {{/each}}
            </ul>
          </div>
        </div>
      {{/each}}
    {{/with}}
    <hr />
    {{! __relaxion__________________________________________________________________________________ }}
    {{#with relaxion}}
      <div class='display-3'>{{title}}</div>
      <div class='col-8'>
        <div class='row g-3'>
          {{#each cards}}
            <div class='col-5'>
              <div class='card text-bg-secondary h-100'>
                <div class='card-header'>
                  {{header}}
                </div>
                <div class='card-body'>
                  <div class='card-title'>
                    {{title}}
                  </div>
                  {{description}}
                </div>
              </div>
            </div>
          {{/each}}
        </div>
      </div>
    {{/with}}
    <hr />
    {{! __rest__________________________________________________________________________________ }}
    {{#with rest}}
      {{#each cards}}
        <div class='col-3'>
          <div class='card h-100 text-center'>
            <div class='card-header'>
              {{header}}
            </div>
            <div class='card-body'>
              <div class='h5 card-title'>
                {{title}}
              </div>
              {{description}}
              <div>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
              </div>
            </div>
            <div class='card-footer'>
              {{footer}}
            </div>
          </div>
        </div>
      {{/each}}
    {{/with}}
    <hr />
    {{! __proposition__________________________________________________________________________________ }}
    {{#with proposition}}
      <p class='display-2'>{{title}}</p>
      {{#each card}}
        <div class='col-3'>
          <div
            class='card gx-2 h-100 text-bg-warning text-light'
          >
            <div class='card-header'>
              {{header}}
            </div>
            <div class='card-body'>
              <div class='card-title h5'>{{title}}</div>
              {{description}}
            </div>
          </div>
        </div>
      {{/each}}
    {{/with}}
    <hr />
    {{! Footer__________________________________________________________________________________ }}
    {{#with footer}}
      <h5>{{title}}</h5>
      <div class='col-2'>
        {{#each navigation}}
          <p>
            <a
              class='text-primary btn btn-outline-light btn-sm'
              href='{{href}}'
              target='_blank'
            >{{text}}</a>
          </p>
        {{/each}}
      </div>
      <div class='col-2'>
        {{#each socialMedia}}
          <p>
            <a
              class='btn btn-outline-dark btn-sm'
              href='{{link}}'
              target='_blank'
            >{{name}}</a>
          </p>
        {{/each}}
      </div>
      <div class='col-2'>
        {{#each partners}}
          <p>
            <a
              class='btn btn-outline-info btn-sm'
              href='{{link}}'
              target='_blank'
            >{{name}}</a>
          </p>
        {{/each}}
      </div>
    {{/with}}
  </section>
</main>