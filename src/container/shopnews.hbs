{{#with newsData}}
  <div class='col-12'>
    <div class='card'>
      <img src='{{photo}}' alt='' />
    </div>
    <h1 class='my-4'>{{head.heading}}</h1>
    <p class='h4 mb-5'>{{head.description}}</p>
    {{#each body}}
      <h2>{{title}}</h2>
      <p class='h5 mb-5'>{{text}}</p>
    {{/each}}
  </div>
{{/with}}
{{#with newsOtherBlock}}
  {{>shoppost}}
{{/with}}