<main class='ms-3'>
  <h2>List of pages</h2>
  <p class='mb-5'>A list of pages that I created during my
    HTML studies</p>
  {{#each page}}
  <div>
    <a
      target='_parent'
      class='btn btn-primary btn-sm mb-2 text-capitalize btn_main'
      href='/{{this}}'
    >{{this}}</a>
  {{/each}}
  </div>
</main>