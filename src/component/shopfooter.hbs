<hr class='my-5' />
<div class='col-12'>
  <div class='row'>
    {{#each footer}}
      <div class='col-2'>
        <div class='nav flex-column'>
          <div class='nav-item'>
            <a
              target='_blank'
              class='nav-link'
              href='{{this.0.link}}'
            >{{this.0.text}}</a>
            <a
              target='_blank'
              class='nav-link'
              href='{{this.1.link}}'
            >{{this.1.text}}</a>
            <a
              target='_blank'
              class='nav-link'
              href='{{this.2.link}}'
            >{{this.2.text}}</a>
            <a
              target='_blank'
              class='nav-link disabled'
              href='{{this.3.link}}'
            >{{this.3.text}}</a>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>